CreditTab:CreateLabel("Auto Cut Tree")
CreditTab:CreateLabel("Game: 99 Nights in the Forest")
CreditTab:CreateLabel("UI: Rayfield")

-- Window
local Window = Rayfield:CreateWindow({
   Name = "99 Nights | Infinite Tree",
   LoadingTitle = "Auto Sapling",
   LoadingSubtitle = "Rayfield UI",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "99Nights",
      FileName = "InfiniteTree"
   },
   KeySystem = false
})

-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LP = Players.LocalPlayer

-- Variables
local AutoPlant = false
local Radius = 15
local Delay = 0.4

-- Tab
local TreeTab = Window:CreateTab("Infinite Tree", 4483362458)

-- Toggle
TreeTab:CreateToggle({
   Name = "Auto Infinite Tree",
   CurrentValue = false,
   Callback = function(Value)
      AutoPlant = Value
   end,
})

-- Radius Slider (BULAT)
TreeTab:CreateSlider({
   Name = "Plant Radius",
   Range = {5, 40},
   Increment = 1,
   CurrentValue = 15,
   Suffix = "Studs",
   Callback = function(Value)
      Radius = Value
   end,
})

-- Delay
TreeTab:CreateSlider({
   Name = "Plant Speed",
   Range = {0.1, 1},
   Increment = 0.1,
   CurrentValue = 0.4,
   Callback = function(Value)
      Delay = Value
   end,
})

-- Cari sapling
local function GetSapling()
   local char = LP.Character
   if not char then return nil end

   for _, v in pairs(char:GetChildren()) do
      if v:IsA("Tool") and string.find(v.Name:lower(), "sap") then
         return v
      end
   end
   return nil
end

-- Loop Infinite Plant
task.spawn(function()
   while task.wait(Delay) do
      if AutoPlant then
         local char = LP.Character
         local sapling = GetSapling()
         if not char or not sapling then continue end

         local hrp = char:FindFirstChild("HumanoidRootPart")
         if not hrp then continue end

         -- Tanam MELINGKAR
         for i = 1, 6 do
            local angle = math.rad((360 / 6) * i)
            local offset = Vector3.new(
               math.cos(angle) * Radius,
               0,
               math.sin(angle) * Radius
            )

            pcall(function()
               sapling:Activate()
               sapling.Handle.CFrame = hrp.CFrame + offset
            end)
         end
      end
   end
end)

-- Credits
local CreditTab = Window:CreateTab("Credits", 4483362458)
CreditTab:CreateLabel("Infinite Tree Sapling")
CreditTab:CreateLabel("Circle Plant Area")
CreditTab:CreateLabel("Rayfield UI")

-- Window
local Window = Rayfield:CreateWindow({
   Name = "99 Nights | Campfire",
   LoadingTitle = "Auto Fuel Campfire",
   LoadingSubtitle = "Rayfield UI",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "99Nights",
      FileName = "CampfireFuel"
   },
   KeySystem = false
})

-- Services
local Players = game:GetService("Players")
local LP = Players.LocalPlayer

-- Variables
local AutoFuel = false
local SelectedFuel = "Wood"
local FuelDelay = 0.5

-- Tab
local FireTab = Window:CreateTab("Campfire", 4483362458)

-- Toggle Auto Fuel
FireTab:CreateToggle({
   Name = "Auto Add Fuel",
   CurrentValue = false,
   Callback = function(Value)
      AutoFuel = Value
   end,
})

-- Dropdown Fuel
FireTab:CreateDropdown({
   Name = "Select Fuel",
   Options = {"Wood", "Log", "Plank"},
   CurrentOption = {"Wood"},
   Callback = function(Option)
      SelectedFuel = Option[1]
   end,
})

-- Speed
FireTab:CreateSlider({
   Name = "Fuel Speed",
   Range = {0.2, 2},
   Increment = 0.1,
   CurrentValue = 0.5,
   Callback = function(Value)
      FuelDelay = Value
   end,
})

-- Cari campfire terdekat
local function GetCampfire()
   local char = LP.Character
   if not char or not char:FindFirstChild("HumanoidRootPart") then return nil end
   local hrp = char.HumanoidRootPart

   local closest, dist = nil, math.huge

   for _, v in pairs(workspace:GetDescendants()) do
      if v:IsA("Model") and string.find(v.Name:lower(), "fire") then
         local part = v:FindFirstChildWhichIsA("BasePart")
         if part then
            local d = (part.Position - hrp.Position).Magnitude
            if d < dist and d < 20 then
               dist = d
               closest = v
            end
         end
      end
   end

   return closest
end

-- Cari fuel di backpack / character
local function GetFuel()
   local char = LP.Character
   local backpack = LP:FindFirstChild("Backpack")
   if not backpack or not char then return nil end

   for _, v in pairs(backpack:GetChildren()) do
      if v:IsA("Tool") and string.find(v.Name:lower(), SelectedFuel:lower()) then
         v.Parent = char -- equip
         return v
      end
   end

   for _, v in pairs(char:GetChildren()) do
      if v:IsA("Tool") and string.find(v.Name:lower(), SelectedFuel:lower()) then
         return v
      end
   end

   return nil
end

-- Auto Fuel Loop
task.spawn(function()
   while task.wait(FuelDelay) do
      if AutoFuel then
         local fire = GetCampfire()
         local fuel = GetFuel()

         if fire and fuel then
            pcall(function()
               fuel:Activate()
            end)
         end
      end
   end
end)

-- Credits
local CreditTab = Window:CreateTab("Credits", 4483362458)
CreditTab:CreateLabel("Auto Campfire Fuel")
CreditTab:CreateLabel("Game: 99 Nights in the Forest")
CreditTab:CreateLabel("Rayfield UI")


