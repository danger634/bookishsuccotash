 local Tab = Window:CreateTab("main", 4483362458)
 
 local GodmodeConnection
local function EnableGodmode()
    if GodmodeConnection then return end
    GodmodeConnection = RunService.Heartbeat:Connect(function()
        local char = LP.Character
        if char then
            local hum = char:FindFirstChildOfClass("Humanoid")
            if hum then
                hum.Health = hum.MaxHealth
                hum:SetStateEnabled(Enum.HumanoidStateType.Dead, false)
            end
        end
    end)
end

local function DisableGodmode()
    if GodmodeConnection then
        GodmodeConnection:Disconnect()
        GodmodeConnection = nil
    end
end

PlayerTab:CreateToggle({
    Name = "Godmode Health",
    CurrentValue = false,
    Flag = "GodmodeHealth",
    Callback = function(state)
        if state then
            EnableGodmode()
        else
            DisableGodmode()
        end
    end
})

local Tab = Window:CreateTab("teleport", 6703861799)

local TeleportTab = Window:CreateTab("Teleport expand map")
local TargetPosition = CFrame.new(120, 15, -80)

TeleportTab:CreateButton({
    Name = "teleport stronghold",
    Callback = function()
        local char = LP.Character or LP.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")
        hrp.CFrame = TargetPosition
    end
})

local Tab = Window:CreateTab("funny", 14221664309)

local TeleportTab = Window:CreateTab("Teleport")
local TargetPosition = CFrame.new(120, 15, -80)

local CombatTab = Window:CreateTab("fling player")

-- Fling Player function
local function FlingPlayer(targetName)
    local target = Players:FindFirstChild(targetName)
    if not target or not target.Character then return end
    local hrp = LP.Character and LP.Character:FindFirstChild("HumanoidRootPart")
    local thrp = target.Character:FindFirstChild("HumanoidRootPart")
    if not hrp or not thrp then return end
    
    local oldCF = hrp.CFrame

    hrp.CFrame = thrp.CFrame
    hrp.AssemblyLinearVelocity = Vector3.new(0, 9999, 0)
    hrp.AssemblyAngularVelocity = Vector3.new(9999, 9999, 9999)

    task.wait(0.2)

    hrp.CFrame = oldCF
end

-- Input for Fling Target
local FlingTarget = ""
CombatTab:CreateTextBox({
    Name = "Fling Player",
    PlaceholderText = "Username",
    RemoveTextAfterFocusLost = false,
    Callback = function(text)
        FlingTarget = text
    end
})

-- Window
local Window = Rayfield:CreateWindow({
   Name = "99 Nights | Auto Farm",
   LoadingTitle = "Auto Cut Tree",
   LoadingSubtitle = "Rayfield UI",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = "99Nights",
      FileName = "AutoTree"
   },
   KeySystem = false
})

-- Services
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LP = Players.LocalPlayer

-- Variables
local AutoCut = false
local AttackDelay = 0.2

-- Tab
local FarmTab = Window:CreateTab("Auto Farm", 4483362458)

-- Toggle Auto Cut
FarmTab:CreateToggle({
   Name = "Auto Cut Tree",
   CurrentValue = false,
   Callback = function(Value)
      AutoCut = Value
   end,
})

-- Speed Slider
FarmTab:CreateSlider({
   Name = "Cut Speed",
   Range = {0.1, 1},
   Increment = 0.1,
   CurrentValue = 0.2,
   Callback = function(Value)
      AttackDelay = Value
   end,
})

-- Function cari kapak
local function GetAxe()
   local char = LP.Character
   if not char then return nil end

   for _, v in pairs(char:GetChildren()) do
      if v:IsA("Tool") then
         return v
      end
   end
   return nil
end

-- Function cari tree terdekat
local function GetNearestTree()
   local char = LP.Character
   if not char or not char:FindFirstChild("HumanoidRootPart") then return nil end
   local hrp = char.HumanoidRootPart

   local closest, dist = nil, math.huge

   for _, v in pairs(workspace:GetDescendants()) do
      if v:IsA("BasePart") and string.find(v.Name:lower(), "tree") or string.find(v.Name:lower(), "trunk") then
         local d = (v.Position - hrp.Position).Magnitude
         if d < dist and d < 25 then
            dist = d
            closest = v
         end
      end
   end

   return closest
end

-- Loop Auto Cut
task.spawn(function()
   while task.wait(AttackDelay) do
      if AutoCut then
         local axe = GetAxe()
         local tree = GetNearestTree()

         if axe and tree then
            pcall(function()
               axe:Activate()
            end)
         end
      end
   end
end)

-- Credits
